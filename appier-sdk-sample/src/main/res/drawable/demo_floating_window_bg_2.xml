<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Paragraph 1 -->

    <item
        android:gravity="left"
        android:top="0dp"
        android:left="0dp"
        android:right="0dp"
        android:height="12dp">
        <shape android:shape="rectangle">
            <solid android:color="#F2F2F2" />
        </shape>
    </item>

    <!-- Paragraph 2 -->

    <item
        android:gravity="left"
        android:top="16dp"
        android:left="0dp"
        android:right="0dp"
        android:height="12dp">
        <shape android:shape="rectangle">
            <solid android:color="#F2F2F2" />
        </shape>
    </item>

    <!-- Main image -->

    <item
        android:gravity="fill_horizontal"
        android:top="36dp"
        android:left="0dp"
        android:right="0dp"
        android:height="86dp">
        <shape android:shape="rectangle">
            <solid android:color="#F2F2F2" />
        </shape>
    </item>

</layer-list>
